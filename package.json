{
  "name": "frontend-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "init": "ng g @moleculemaker/ui-lib:deployment && npm run openapi:live",
    
    "openapi:clean": "rm -rf src/app/api/",
    "openapi:mmli": "openapi-generator-cli generate --skip-validate-spec -i openapi/mmli-backend.openapi.json -g typescript-angular -o src/app/api/mmli-backend/v1",
    "openapi:mmli:sync": "curl https://mmli.fastapi.staging.mmli1.ncsa.illinois.edu/openapi.json > openapi/mmli-backend.openapi.json && cat openapi/mmli-backend.openapi.json",
    "openapi:mmli:live": "npm run openapi:mmli:sync && npm run openapi:mmli",
    "openapi:mmli:sync:local": "curl http://localhost:8080/openapi.json > openapi/mmli-backend.openapi.json && cat openapi/mmli-backend.openapi.json",
    "openapi:mmli:live:local": "npm run openapi:mmli:sync:local && npm run openapi:mmli",

    "openapi:moldb": "openapi-generator-cli generate --skip-validate-spec -i openapi/moldb.openapi.json -g typescript-angular -o src/app/api/moldb/v1",
    "openapi:moldb:sync": "curl https://postgrest.openenzymedb.mmli1.ncsa.illinois.edu/ > openapi/moldb.openapi.json && cat openapi/moldb.openapi.json",
    "openapi:moldb:live": "npm run openapi:moldb:sync && npm run openapi:moldb",
    "openapi:moldb:sync:local": "curl http://localhost:4000 > openapi/moldb.openapi.json && cat openapi/moldb.openapi.json",
    "openapi:moldb:live:local": "npm run openapi:moldb:sync:local && npm run openapi:moldb",

    "openapi": "npm run openapi:mmli && npm run openapi:moldb",
    "openapi:live": "npm run openapi:mmli:live && npm run openapi:moldb:live",
    "openapi:live:local": "npm run openapi:mmli:live:local && npm run openapi:moldb:live:local",
    "openapi:sync": "npm run openapi:mmli:sync && npm run openapi:moldb:sync",
    "openapi:sync:local": "npm run openapi:mmli:sync:local && npm run openapi:moldb:sync:local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.7",
    "@angular/common": "^18.2.7",
    "@angular/compiler": "^18.2.7",
    "@angular/core": "^18.2.7",
    "@angular/forms": "^18.2.7",
    "@angular/platform-browser": "^18.2.7",
    "@angular/platform-browser-dynamic": "^18.2.7",
    "@angular/router": "^18.2.7",
    "@chemaxon/marvinjs": "^23.11.0",
    "@moleculemaker/dev-tool": "^0.0.6",
    "chart.js": "^3",
    "cypress": "^13.14.2",
    "d3": "^7.9.0",
    "driver.js": "^1.3.1",
    "ngx-matomo-client": "^6.3.1",
    "pako": "^2.1.0",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.11",
    "rxjs": "~7.8.0",
    "sunburst-chart": "^1.21.1",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.7",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.15.3",
    "@types/pako": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.4.5"
  }
}
